{% extends 'base.html.twig' %}

{% block title %}CourseType{% endblock %}

{% block body %}
    <h1>CourseType</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ course_type.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ course_type.name }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ course_type.description }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_course_type_index') }}">{% trans %}back to list{% endtrans %}</a>

    <a href="{{ path('app_course_type_edit', {'id': course_type.id}) }}">{% trans %}edit{% endtrans %}</a>

    {{ include('course_type/_delete_form.html.twig') }}
{% endblock %}
